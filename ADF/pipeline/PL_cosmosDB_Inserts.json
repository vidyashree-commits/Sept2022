					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "PO_NUMBER",
									"type": "String"
								},
								"sink": {
									"path": "$['PO_NUMBER']"
								}
							},
							{
								"source": {
									"name": "AGENCY",
									"type": "String"
								},
								"sink": {
									"path": "$['AGENCY']"
								}
							},
							{
								"source": {
									"name": "COMMODITY",
									"type": "String"
								},
								"sink": {
									"path": "$['COMMODITY']"
								}
							},
							{
								"source": {
									"name": "VENDOR_NAME",
									"type": "String"
								},
								"sink": {
									"path": "$['VENDOR_NAME']"
								}
							},
							{
								"source": {
									"name": "ORDERED_DATE",
									"type": "String"
								},
								"sink": {
									"path": "$['ORDERED_DATE']"
								}
							},
							{
								"source": {
									"name": "PO_AMOUNT",
									"type": "String"
								},
								"sink": {
									"path": "$['PO_AMOUNT']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Forcosmosdborders",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}